{
  "name": "ourmusic-backend",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "start": "bun src/index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "check": "find src -name '*.js' | xargs bun check && bun test && bun run lint",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "eslint": "^9.22.0",
    "sequelize-cli": "^6.6.2"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "elysia": "^1.2.25",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.6"
  }
}
